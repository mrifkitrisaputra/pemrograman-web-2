[
    {
      "name": "Directory Listing",
      "icon": "https://img.icons8.com/color/48/folder-invoices.png",
      "query": "intitle:index.of",
      "type": "google"
    },
    {
      "name": "Configuration Files",
      "icon": "https://img.icons8.com/color/48/settings--v1.png",
      "query": "ext:xml | ext:conf | ext:cnf | ext:reg | ext:inf | ext:rdp | ext:cfg | ext:txt | ext:ora | ext:ini",
      "type": "google"
    },
    {
      "name": "Database Files",
      "icon": "https://img.icons8.com/color/48/database--v1.png",
      "query": "ext:sql | ext:dbf | ext:mdb",
      "type": "google"
    },
    {
      "name": "WordPress",
      "icon": "https://img.icons8.com/color/48/wordpress.png",
      "query": "inurl:wp- | inurl:wp-content | inurl:wp-includes | inurl:plugins | inurl:uploads | inurl:themes | inurl:download",
      "type": "google"
    },
    {
      "name": "Log Files",
      "icon": "https://img.icons8.com/?size=100&id=dlPq99z6DwTb&format=png&color=000000",
      "query": "ext:log",
      "type": "google"
    },
    {
      "name": "Backup and Old Files",
      "icon": "https://img.icons8.com/?size=100&id=31477&format=png&color=000000",
      "query": "ext:bkf | ext:bkp | ext:bak | ext:old | ext:backup",
      "type": "google"
    },
    {
      "name": "Login Pages",
      "icon": "https://img.icons8.com/color/48/login-rounded-right.png",
      "query": "inurl:login | inurl:signin | intitle:Login | intitle: signin | inurl:auth",
      "type": "google"
    },
    {
      "name": "SQL Errors",
      "icon": "https://img.icons8.com/color/48/error--v1.png",
      "query": "intext:\"sql syntax near\" | intext:\"syntax error has occurred\" | intext:\"incorrect syntax near\" | intext:\"unexpected end of SQL command\" | intext:\"Warning: mysql_connect()\" | intext:\"Warning: mysql_query()\" | intext:\"Warning: pg_connect()\"",
      "type": "google"
    },
    {
      "name": "Apache Config Files",
      "icon": "https://img.icons8.com/?size=100&id=OrZMxhqm7nsr&format=png&color=000000",
      "query": "filetype:config \"apache\"",
      "type": "google"
    },
    {
      "name": "Robots.txt File",
      "icon": "https://img.icons8.com/?size=100&id=0pTIwwYcK7dy&format=png&color=000000",
      "query": "/robots.txt",
      "type": "google"
    },
    {
      "name": "Whois",
      "icon": "https://img.icons8.com/color/48/whois.png",
      "urlTemplate": "https://www.whois.com/whois/{targetDomain}",
      "type": "custom"
    },
    {
      "name": "Publicly Exposed Documents",
      "icon": "https://img.icons8.com/color/48/documents.png",
      "query": "ext:doc | ext:docx | ext:odt | ext:pdf | ext:rtf | ext:sxw | ext:psw | ext:ppt | ext:pptx | ext:pps | ext:csv",
      "type": "google"
    },
    {
      "name": "phpinfo()",
      "icon": "https://img.icons8.com/?size=100&id=121463&format=png&color=000000",
      "query": "ext:php intitle:phpinfo \"published by the PHP Group\"",
      "type": "google"
    },
    {
      "name": "Finding Backdoor",
      "icon": "https://img.icons8.com/?size=100&id=7msJIrmR4mOD&format=png&color=000000",
      "query": "inurl:shell | inurl:backdoor | inurl:wso | inurl:cmd | shadow | passwd | boot.ini | inurl:backdoor",
      "type": "google"
    },
    {
      "name": "Install / Setup files",
      "icon": "https://img.icons8.com/color/48/installing-updates.png",
      "query": "inurl:readme | inurl:license | inurl:install | inurl:setup | inurl:config",
      "type": "google"
    },
    {
      "name": "Open Redirects",
      "icon": "https://img.icons8.com/?size=100&id=5bt0dHg2NBNZ&format=png&color=000000",
      "query": "inurl:redir | inurl:url | inurl:redirect | inurl:return | inurl:src=http | inurl:r=http",
      "type": "google"
    },
    {
      "name": "Apache STRUTS RCE",
      "icon": "https://img.icons8.com/?size=100&id=12Xk5enJGiNJ&format=png&color=000000",
      "query": "ext:action | ext:struts | ext:do",
      "type": "google"
    },
    {
      "name": "3rd Party Exposure",
      "icon": "https://img.icons8.com/?size=100&id=108349&format=png&color=000000",
      "urlTemplate": "https://www.google.com/search?q=site:http://ideone.com | site:http://codebeautify.org | site:http://codeshare.io | site:http://codepen.io | site:http://repl.it | site:http://justpaste.it | site:http://pastebin.com | site:http://jsfiddle.net | site:http://trello.com | site:*.atlassian.net | site:bitbucket.org+\"{targetDomain}\"",
      "type": "custom"
    },
    {
      "name": "Check Security Headers",
      "icon": "https://img.icons8.com/color/48/security-checked.png",
      "urlTemplate": "https://securityheaders.com/?q={targetDomain}&followRedirects=on",
      "type": "custom"
    },
    {
      "name": "Gitlab",
      "icon": "https://img.icons8.com/color/48/gitlab.png",
